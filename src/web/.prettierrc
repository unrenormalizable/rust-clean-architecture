{
  "trailingComma": "none",
  "tabWidth": 2,
  "printWidth": 120,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "endOfLine": "crlf",
  "tailwindConfig": "./tailwind.config.js",
  "tailwindStylesheet": "./src/index.css",
  "plugins": ["prettier-plugin-tailwindcss"]
}
